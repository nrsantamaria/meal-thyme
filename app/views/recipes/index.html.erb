<div class="container">
  <h2>Recipes</h2>
  <% @recipes.each do |recipe| %>
    <div class="panel panel-default">
      <div class="panel-heading">
        <h3><%= link_to recipe.title, recipe_path(recipe.id) %></h3>
      </div>
      <div class="panel-body">
        <%= image_tag(recipe.imageUrl, size: "400x400",  alt:'recipe image', id:'recipe') %>
      </div>
    </div>
  <% end %>
</div>
